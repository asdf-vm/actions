{
  "name": "@asdf-vm/actions",
  "version": "2.0.0",
  "description": "asdf github actions",
  "repository": "https://github.com/asdf-vm/actions",
  "author": "Victor Borja <vborja@apache.org>",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "lefthook install",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "build": "esbuild src/install/main.ts src/plugin-test/main.ts src/plugins-add/main.ts src/setup/main.ts --bundle --outdir=. --target=node16 --platform=node"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0",
    "@actions/io": "1.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.2",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "esbuild": "^0.17.11",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  }
}
